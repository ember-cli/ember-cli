    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-n": "^16.0.1",
    "fixturify": "^3.0.0",
    "jsdom": "^21.1.1",
    "latest-version": "^9.0.0",
    "mocha": "^11.1.0",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "prettier": "3.7.4",
