    "diff": "^8.0.2",
    "ember-cli-is-package-missing": "^1.0.0",
    "ember-cli-normalize-entity-name": "^1.0.0",
    "ember-cli-preprocess-registry": "^5.0.1",
    "ember-cli-string-utils": "^1.1.0",
    "ensure-posix-path": "^1.1.1",
    "execa": "^5.1.1",
    "exit": "^0.1.2",
