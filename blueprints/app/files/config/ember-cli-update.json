{
  "schemaVersion": "1.0.0",
  "packages": [
    {
      "name": "ember-cli",
      "version": "<%= emberCLIVersion %>",
      "blueprints": [
        {
          "name": "<%= blueprint %>",
          "outputRepo": "https://github.com/ember-cli/ember-<%= blueprint === 'app' ? 'new' : 'addon' %>-output",
          "codemodsSource": "ember-<%= blueprint %>-codemods-manifest@1",
          "isBaseBlueprint": true,
          "options": [<%= blueprintOptions %>]
        }
      ]
    }
  ]
}
