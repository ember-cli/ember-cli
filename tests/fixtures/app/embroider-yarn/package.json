    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^15.15.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^36.0.1",
    "tracked-built-ins": "^3.4.0",
